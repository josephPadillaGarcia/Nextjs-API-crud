generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Worker {
  idworker Int @id @default(autoincrement())
  nombre String @db.VarChar(255)
  apellido String @db.VarChar(255)
  edad Int
  dni Int
  points Int
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt
}